{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "outputDirectory": "public",
  
  "headers": [
    {
      "source": "/(.*\\.js)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript; charset=utf-8" }
      ]
    },
    {
      "source": "/(.*\\.css)",
      "headers": [
        { "key": "Content-Type", "value": "text/css; charset=utf-8" }
      ]
    }
  ],
  
  "rewrites": [
    { "source": "/", "destination": "/welcome.html" },
    { "source": "/welcome", "destination": "/welcome.html" },
    { "source": "/signin", "destination": "/signin.html" },
    { "source": "/signup", "destination": "/signup.html" },
    { "source": "/hi-island", "destination": "/hi-island-NEW.html" },
    { "source": "/hi-muscle", "destination": "/hi-muscle.html" },
    { "source": "/hi-dashboard", "destination": "/hi-dashboard.html" }
  ],
  
  "redirects": [
    {
      "source": "/hi-island.html", 
      "destination": "/hi-island",
      "permanent": true
    }
  ]
}